{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/keyboard": [
      "default",
      {
        "nested-interactive": "off"
      }
    ],
    "button-type": "off",
    "no-inline-styles": "off",
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off",
        "button-name": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "axe/structure": [
      "default",
      {
        "list": "off",
        "listitem": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "backdrop-filter",
          "contain",
          "scale"
        ]
      }
    ],
    "axe/parsing": [
      "default",
      {
        "duplicate-id": "off",
        "duplicate-id-active": "off"
      }
    ],
    "css-prefix-order": "off",
    "detect-css-reflows/composite": "off"
  }
}